<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="180.0" prefWidth="416.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DoYouWanToSaveChangeController">
   <children>
      <Label layoutX="64.0" layoutY="53.0" prefHeight="32.0" prefWidth="309.0" text="Do you want to save change?">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Button layoutX="94.0" layoutY="114.0" mnemonicParsing="false" text="Save" fx:id = "ConfirmSaveButton" onAction="#SaveContent"/>
      <Button layoutX="163.0" layoutY="114.0" mnemonicParsing="false" text="Don't save" fx:id = "DontSaveButton" onAction="#DontSave"/>
      <Button layoutX="271.0" layoutY="114.0" mnemonicParsing="false" text="Cancel" fx:id = "CancelButton" onAction="#Cancel"/>
   </children>
</AnchorPane>
